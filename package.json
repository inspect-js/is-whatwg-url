{
	"name": "is-whatwg-url",
	"version": "0.0.0",
	"description": "Is this value a WHATWG-standard URL object?",
	"main": "index.js",
	"exports": {
		".": "./index.js",
		"./package.json": "./package.json"
	},
	"types": "./index.d.ts",
	"scripts": {
		"prepack": "npmignore --auto --commentLines=autogenerated",
		"prepublishOnly": "safe-publish-latest",
		"prepublish": "not-in-publish || npm run prepublishOnly",
		"pretest": "npm run --silent lint",
		"test": "npm run tests-only",
		"tests-only": "nyc tape 'test/**/*.js'",
		"posttest": "aud --production",
		"prelint": "evalmd README.md",
		"lint": "eslint --ext=js,mjs .",
		"postlint": "tsc -p . && attw -P",
		"version": "auto-changelog && git add CHANGELOG.md",
		"postversion": "auto-changelog && git add CHANGELOG.md && git commit --no-edit --amend && git tag -f \"v$(node -e \"console.log(require('./package.json').version)\")\""
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/inspect-js/is-whatwg-url.git"
	},
	"keywords": [
		"url",
		"whatwg",
		"web",
		"html"
	],
	"author": "Jordan Harband <ljharb@gmail.com>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/inspect-js/is-whatwg-url/issues"
	},
	"homepage": "https://github.com/inspect-js/is-whatwg-url#readme",
	"dependencies": {
		"call-bind": "^1.0.7",
		"gopd": "^1.0.1"
	},
	"devDependencies": {
		"@arethetypeswrong/cli": "^0.15.3",
		"@ljharb/eslint-config": "^21.1.1",
		"@ljharb/tsconfig": "^0.2.0",
		"@types/call-bind": "^1.0.5",
		"@types/es-value-fixtures": "^1.4.4",
		"@types/for-each": "^0.3.3",
		"@types/gopd": "^1.0.3",
		"@types/has-bigints": "^1.0.2",
		"@types/has-symbols": "^1.0.2",
		"@types/make-arrow-function": "^1.2.2",
		"@types/make-async-function": "^1.0.2",
		"@types/make-generator-function": "^2.0.3",
		"@types/node": "^6.14.13",
		"@types/object-inspect": "^1.13.0",
		"@types/object.assign": "^4.1.0",
		"@types/semver": "^7.5.8",
		"@types/tape": "^5.6.4",
		"aud": "^2.0.4",
		"es-value-fixtures": "^1.4.2",
		"eslint": "=8.8.0",
		"evalmd": "^0.0.19",
		"for-each": "^0.3.3",
		"in-publish": "^2.0.1",
		"npmignore": "^0.3.1",
		"nyc": "^10.3.2",
		"object-inspect": "^1.13.1",
		"safe-publish-latest": "^2.0.0",
		"semver": "^6.3.1",
		"tape": "^5.7.5",
		"typescript": "next"
	},
	"engines": {
		"node": ">= 0.4"
	},
	"auto-changelog": {
		"output": "CHANGELOG.md",
		"template": "keepachangelog",
		"unreleased": false,
		"commitLimit": false,
		"backfillLimit": false,
		"hideCredit": true
	},
	"publishConfig": {
		"ignore": [
			".github/workflows"
		]
	},
	"testling": {
		"files": "test/index.js"
	}
}
